begin
       set i = 1
       set max_elem = a
       display "initial max_elem:" max_elem
       repeat 
           if a > max_elem
           then
               set max_elem = a
               set h = varm2 * 3
               display "max_elem:" max_elem
           endif
           set i = i + 1
       until
       return max_elem
endfun max_array